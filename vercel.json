{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(login\\.html|index\\.html|admin\\.html|integrations\\.html|auth-handler\\.js)",
      "dest": "/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/exact-search",
      "dest": "/server.js"
    },
    {
      "src": "/search",
      "dest": "/server.js"
    },
    {
      "src": "/search-by-line",
      "dest": "/server.js"
    },
    {
      "src": "/search-by-ticket",
      "dest": "/server.js"
    },
    {
      "src": "/generate-official-response",
      "dest": "/server.js"
    },
    {
      "src": "/health",
      "dest": "/server.js"
    },
    {
      "src": "/data-sample",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}